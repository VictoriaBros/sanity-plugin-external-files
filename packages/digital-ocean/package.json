{
  "name": "@victoriabros/sanity-digital-ocean-dam-plugin",
  "version": "2.0.1",
  "description": "Store Sanity media files in Digital Ocean Spaces",
  "scripts": {
    "clear-lib": "node clearLib.js",
    "build": "npm run clear-lib && tsc && tsc --module esnext --outDir build/esm",
    "dev": "concurrently \"tsc -w\" \"tsc --module esnext --outDir build/esm -w\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/victoriabros/sanity-plugin-external-files.git"
  },
  "keywords": [
    "sanity.io",
    "sanity",
    "media",
    "Digital Ocean Spaces",
    "asset management",
    "sanity-plugin"
  ],
  "author": "Henrique Doro <opensource@hdoro.dev>",
  "license": "Apache-2.0",
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@victoriabros/sanity-external-dam-plugin": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^4.10.3",
    "styled-components": "^6.1.15"
  },
  "peerDependencies": {
    "sanity": ">=4.0.0",
    "styled-components": "^6"
  },
  "bugs": {
    "url": "https://github.com/victoriabros/sanity-plugin-external-files/issues"
  },
  "homepage": "https://github.com/victoriabros/sanity-plugin-external-files#readme",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/esm/index.js",
      "require": "./build/index.js",
      "default": "./build/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts"
}
